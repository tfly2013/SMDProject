<h1>Listing events</h1>

<table>
  <thead>
    <tr>
      <th>Society</th>
      <th>Group</th>
      <th>Name</th>
      <th>Type</th>
      <th>Time</th>
      <th>Location</th>
      <th>Webpage</th>
      <th>Condition</th>
      <th>Description</th>
      <th>Status</th>
      <th>String</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.society_id %></td>
        <td><%= event.group_id %></td>
        <td><%= event.name %></td>
        <td><%= event.type %></td>
        <td><%= event.time %></td>
        <td><%= event.location %></td>
        <td><%= event.webpage %></td>
        <td><%= event.condition %></td>
        <td><%= event.description %></td>
        <td><%= event.status %></td>
        <td><%= event.string %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>

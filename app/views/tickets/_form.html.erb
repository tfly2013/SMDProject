<%= form_for(@ticket) do |f| %>
  <% if @ticket.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@ticket.errors.count, "error") %> prohibited this ticket from being saved:</h2>

      <ul>
      <% @ticket.errors.full_messages.each do |msg| %>
        <li><%= msg %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :event_id %><br>
    <%= f.number_field :event_id %>
  </div>
  <div class="field">
    <%= f.label :price %><br>
    <%= f.text_field :price %>
  </div>
  <div class="field">
    <%= f.label :purchase_location %><br>
    <%= f.text_field :purchase_location %>
  </div>
  <div class="field">
    <%= f.label :strat_time %><br>
    <%= f.datetime_select :strat_time %>
  </div>
  <div class="field">
    <%= f.label :valid_time %><br>
    <%= f.datetime_select :valid_time %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>

